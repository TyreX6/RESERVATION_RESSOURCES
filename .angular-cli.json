{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "reservation-ressources"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "manifest.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "assets/plugins/bootstrap/css/bootstrap.css",
        "assets/plugins/node-waves/waves.css",
        "assets/plugins/animate-css/animate.css",
        "assets/css/style.css",
        "assets/css/themes/all-themes.css",
        "assets/calendar/dist/fullcalendar.css",
        "assets/adminbag/assets/css/skins/all.css",
        "assets/adminbag/assets/css/buttons.css",
        "assets/adminbag/assets/css/main.css",
        "assets/css/personalized.css",
        "assets/adminbag/assets/css/select2.min.css",
        "../node_modules/ngx-toastr/toastr.css",
        "assets/css/picker.min.css",
        "styles.css"
      ],
      "scripts": [
        "assets/plugins/jquery/jquery.min.js",
        "assets/plugins/bootstrap/js/bootstrap.js",
        "assets/plugins/jquery-slimscroll/jquery.slimscroll.js",
        "assets/plugins/node-waves/waves.js",
        "assets/js/admin.js",
        "assets/adminbag/assets/js/vendor/icheck.js",
        "../node_modules/moment/moment.js",
        "../node_modules/fullcalendar/dist/fullcalendar.min.js",
        "assets/adminbag/assets/js/vendor/lib/jquery-ui.custom.min.js",
        "assets/adminbag/assets/js/vendor/select2.min.js",
        "assets/adminbag/assets/js/main.js",
        "assets/js/modals.js",
        "assets/js/bootstrap-material-datetimepicker.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
